{
  "name": "@clavion/cli",
  "version": "0.1.0",
  "description": "Key management CLI for import, generation, and listing",
  "author": "Dmitri N",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clavion-xyz/clavion.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/clavion-xyz/clavion",
  "bugs": "https://github.com/clavion-xyz/clavion/issues",
  "keywords": [
    "clavion",
    "iscl",
    "cli",
    "key-management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
